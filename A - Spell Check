#include<bits/stdc++.h>
#include<algorithm>
using namespace std;
int main() 
{
    int t;
    cin>>t;
    while (t--) 
    {
        int n;
        cin>>n;
        string a;
        cin>>a;
        sort(a.begin(),a.end());
        string ab="Timur";
        sort(ab.begin(),ab.end());
        if(ab==a)
        {
            cout<<"YES"<<endl;
        }
        else
        {
            cout<<"NO"<<endl;
        }
    }
    return 0;
}
