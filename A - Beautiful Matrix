#include<bits/stdc++.h>
#include<cmath>
using namespace std;
int main()
{
    int t[5][5];
    int m=0;
    for(int i=0;i<5;i++)
    {
        for(int j=0;j<5;j++)
        {
            cin>>t[i][j];
            if(t[i][j]==1)
          {
            m=abs(i-2)+abs(j-2);
          }
          
        }

    }
    cout<<m<<endl;
    return 0;
}
