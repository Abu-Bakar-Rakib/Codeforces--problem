#include <bits/stdc++.h>
#include <string>
using namespace std;
int main() 
{
    int t;
    cin>>t;
    while (t--) 
    {
        string tt;
        cin>>tt;
        int s1 =0,s2 =0;
        for(int i =0;i < 3; i++) 
        {
            s1 += tt[i] - '0';
            s2 += tt[i + 3] - '0';
        }

        if (s1 == s2) 
        {                           
            cout<<"YES"<< endl;
        } 
        else 
        {
            cout<<"NO"<< endl;
        }
    }
    return 0;
}
