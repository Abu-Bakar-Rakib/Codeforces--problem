#include <bits/stdc++.h>
#include <iomanip>
using namespace std;
int main() 
{
    int n;
    cin>>n;
    double totalv=0;
    for(int i=0;i<n;i++)
    {
        int v;
        cin>>v;
        totalv=totalv+v;
    }
    double vf=(totalv/n);
    cout<<fixed<<setprecision(12)<<vf<<endl;
    return 0;
}
