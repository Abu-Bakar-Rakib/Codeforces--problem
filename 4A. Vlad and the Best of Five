#include<iostream>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        string s;
        cin>>s;
        int ac=0,bc=0;
        for(char ch: s)
        {
            if(ch=='A')
            {
                ac++;
            }
            else
            {
                bc++;
            }
        }
        if(ac>bc)
        {
            cout<<"A"<<endl;
        }
        else
        {
            cout<<"B"<<endl;
        }
    }
    return 0;
}
