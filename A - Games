#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int ab=0;
    vector<int>a1;
    vector<int>a2;
    for (int i = 0; i < n; i++)
    {
        int x,y;
        cin>>x>>y;
        a1.push_back(x);
        a2.push_back(y);
    }
    for (int i = 0; i < n ; i++)
    {
        for (int j = 0; j < n; j++)
        {
            if(a1[i]== a2[j])
            {
                ab++;
            }
        }
    }
    cout<<ab<<endl;
    return 0;
}
