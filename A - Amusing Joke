#include<iostream>
#include<string>
#include<algorithm>
using namespace std;
int main()
{
    string gn,rn,fd;
    cin>>gn>>rn>>fd;
    string ab= gn+rn;
    sort (ab.begin(),ab.end());
    sort (fd.begin(),fd.end());
    if(fd==ab)
    {
        cout<<"YES"<<endl;
    }
    else
    {
        cout<<"NO"<<endl;
    }
    return 0;
}
