#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    int as=0,bs=0,cs=0;
    while (t--)
    {

        int a,b,c;
        cin>>a>>b>>c;
        as+=a;
        bs+=b;   //// maraaaaaaaaaaaaaaaaaaa  ///
        cs+=c;
    }

    if(as==0 && bs==0 && cs==0)
    {

        cout<<"YES"<<endl;
    }
    else
    {
        cout<<"NO"<<endl;
    } //agun lagai dibooo//
    return 0;
}
