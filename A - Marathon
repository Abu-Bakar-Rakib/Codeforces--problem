#include <bits/stdc++.h>
using namespace std;
int main() 
{
    int t;
    cin>>t;
    while(t--)
    {
       int a,b,c,d;
       cin>>a>>b>>c>>d;
       int x=0;
       if(a<b)
       {
           x++;
       }
       if(a<c)
       {
           x++;
       }
       if(a<d)
       {
           x++;
       }
       cout<<x<<endl;
    }
    return 0;
}
