#include<bits/stdc++.h>
using namespace std;
int main()
{
   int t;
   cin>>t;
   while(t--)
   {
       string a;
       cin>>a;
       string ab="codeforces";
       int count=0;
       for(int i=0;i<10;i++)
       {
           if(a[i]!=ab[i])
           {
               count++;
           }
       }
       cout<<count<<endl;
   }
   return 0;
}
