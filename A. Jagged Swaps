#include<bits/stdc++.h>
#include<vector>
using namespace std;
int main()
{
  int t;
  cin>>t;
  while(t--)
  {
    int n;
    cin>>n;
    vector<int>l(n);
    for(int i=0;i<l.size();i++)
     {
        cin>>l[i];
     }
         bool ab=0;
    if(l[0]!=1)
     {
         ab=1;
     }
    if(ab==0)
     {
        cout<<"YES"<<endl;
     }
      else
     {
        cout<<"NO"<<endl;
     }
  
  }
  return 0;
}
