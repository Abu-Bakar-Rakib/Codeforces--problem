#include<bits/stdc++.h>
using namespace std;
int main() 
{
    int t;
    cin>>t;
    while(t--) 
    {
        int n;
        cin>>n;
        int count=0;
        if(n%3 == 0) 
        {
            count++;
            cout<<"Second"<<endl;
        } 
        else 
        {
            cout<<"First"<<endl;
        }
        if(count>=10)
        {
            cout<<"First"<<endl;
        }
    }
    return 0;
}
