#include<bits/stdc++.h>
#include<string>
using namespace std;
int main() 
{
    string n1,n2;
    cin>>n1>>n2;
    string s;
    for(int i=0;i<n1.length();i++) 
    {
        if(n1[i] != n2[i]) 
        {
            cout<<"1"<<""; 
        } 
        else 
        {
            cout<<"0"<<"";
        }
    }
    return 0;
}
